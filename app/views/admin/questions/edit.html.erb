<%= render 'admin/shared/header' %>

<% @title = 'お題編集' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for [ :admin, @question ] do |f| %>
    <%= render 'form', f: f %>
    <div class="buttons">
      <%= f.submit '更新' %>
      <%= link_to 'キャンセル', :admin_questions %>
    </div>
  <% end %>
</div>
