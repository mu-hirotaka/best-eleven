<%= render 'admin/shared/header' %>

<% @title = 'メンテナンス情報編集' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for @maintenance, :url => { :action => 'update' } do |f| %>
    <%= render 'form', f: f %>
    <div class="buttons">
      <%= f.submit '更新' %>
      <%= link_to 'キャンセル', :admin_maintenances %>
    </div>
  <% end %>
</div>
